//CREME DE BRATTE
actor CremeDeBratte : DDOrigCreature 
{
		//$Category RPMonsters
		Health 150
		Radius 20
		Height 56
		Mass 1000
		Speed 20
		PainChance 256
		DamageFactor "Cricket", 2
		DamageFactor "Chop", 2
		DamageFactor "Fire", 2
		DamageFactor "CherryBomb", 2
		MONSTER
		+FLOORCLIP
		+NOINFIGHTING
		+NOTARGET
		+NOBLOOD
		+DONTHARMCLASS
		+MISSILEMORE
		+MISSILEEVENMORE
		PainSound "tart/pain"
		DeathSound "aqua/kill1"
		Obituary "%o was creamed by a Creme de Bratte."
		Tag "Creme De Bratte"
		Species "DeBratte"
	
	
	States
	{
		Spawn:
			TART AB 10 A_Look
			Loop
		See:
			TART AB 5
			TART C 0 A_PlaySound("tart/hop",CHAN_BODY)
			TART CCDD 3 A_Chase
			TART EAE 2
			TART A 2 
			Loop
		Missile:
			TART H 0 A_DynamicMusic
			TART H 0 A_Playsound("aqua/kill2",1)
			TART HIJ 5
			TART K 12 A_CustomMissile("SeedBall",40,0,0)
			TART H 3
			Goto See
		Pain:
			TART F 3 A_Pain
			TART GFG 3 
			Goto See
		Pain.Fire:
			BART F 3 A_Pain
			BART G 3 A_Die("Fire")
			Goto Death.Fire
		Pain.Cherrybomb:
			BART F 3 A_Pain
			BART G 3 A_Die("Cherrybomb")
			Goto Death.Fire
		Death.Suck:
			TART F 5 A_PlaySound("VacDed",CHAN_BODY)
			TART F 2 A_RadiusGive("VacAmmo",128,RGF_PLAYERS,4)
			Stop
		Death:
			TART F 3 A_Pain
			TART GFG 3 
			DANS R 5 bright A_Scream
			DANS S 3 bright A_NoBlocking
			DANS TUVWXYZ 2 bright
			Stop
		Death.Fire:
		Death.Cherrybomb:
			BART F 3 bright
			BART G 0 A_SpawnItemEx("CremeDeBratte_Torched",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
			Stop
	}
}

