Actor RavenWeapon : Beak
{
	Weapon.SelectionOrder 10000
	+Weapon.DontBob
	+Weapon.MeleeWeapon
	+Weapon.NoAlert
	States
	{
		Ready:
			TNT1 A 1 A_WeaponReady
			Loop
		Deselect:
			TNT1 A 1
			{
				if (CountInv("PowerStrength",AAPTR_PLAYER1) || CountInv("PowerStrengthDD",AAPTR_PLAYER1)) { A_Lower(12); }
				A_Lower(12);
			}
			Loop
		Select:
			TNT1 A 1 A_BeakRaise
			Loop
		Fire:
			TNT1 A 18 A_Punch
			Goto Ready
	}
}

