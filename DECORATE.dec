		Missile:
			ANGA A 0 A_DynamicMusic
			ANGA A 0 A_Jump(128,"DeathSong")
			ANGA A 0 A_Jump(160,"AnnoyingSong")
			ANGA A 0 A_PlaySound("Handmother/Zap")
			ANGA AB 5 bright A_Facetarget
			ANGA C 3 bright A_SpawnProjectile("BadZapShot",48)
			ANGA B 3 bright A_Facetarget
			ANGA C 3 bright A_SpawnProjectile("BadZapShot",48)
			ANGA B 3 bright A_Facetarget
			ANGA C 3 bright A_SpawnProjectile("BadZapShot",48)
			ANGA C 5 bright
			ANGL A 3
			Goto See
		AnnoyingSong:
			ANGL A 0 A_Playsound("angel/ballattack",CHAN_VOICE)
			ANGL A 8 A_Facetarget
			ANGA D 8 A_Playsound("virtuoso/musicfire")
			ANGA D 6 A_SpawnProjectile("MusicBall",48)
			ANGA D 10
			ANGL BA 4
			Goto See
		DeathSong:
			ANGL A 0 A_Playsound("angel/deathsong",CHAN_VOICE)
			ANGL A 8 A_Facetarget
			ANGA D 8
			ANGA DDDDDD 6 A_SpawnProjectile("JorgSong",48)
			ANGA D 10
			ANGL BA 4
			Goto See
