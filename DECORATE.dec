		DropItem "MusicAmmo" 24
		DropItem "MusicAmmo" 6
		DropItem "AppleWine" 96
		DropItem "AppleWine" 6
		
	
	States
	{
		Spawn:
			ANGL C 0 A_SpawnItemEx("Mininote", 0, 0, 25, 0, random(-1, 1), random(-1, 1))
