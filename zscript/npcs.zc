class Chef1 : DDAFDNPC
{
	default
	{
		//$Category EE NPCs
		Health 10;
		Radius 20;
		Height 64;
		Speed 0;
		PainChance 256;
		Mass 5000;
		DamageFactor "Cricket", 3;
		DamageFactor "Lead", 2;
		DropItem "EggAmmo", 64;
		Tag "Chef 1";
	}

	States
	{
		Spawn:
			NAD9 ABCD 10 A_Look();
			Goto See;
		See:
			NAD9 ABCD 10 A_Chase();
			Loop;
		/*
		Death.Suck:
			NAD9 A 5 A_StartSound("VacDed",CHAN_BODY);
			NAD9 A 2 A_RadiusGive("VacAmmo",128,RGF_PLAYERS,3);
