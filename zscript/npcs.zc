	{
		super.PostBeginPlay();
		actor player = players[0].mo;
		if (player.CountInv("IsCarnOfSouls") && level.levelnum == 42) // If it's Drake, remove COUNTKILL
		{
			if (bCOUNTKILL)
			{
				bCOUNTKILL = false;
				level.total_monsters -= 1;
			}
