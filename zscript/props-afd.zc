class Centaurion_Hedge : EEProp
{
	default
	{
		//$Angled
		//$Color 12
		//$Category "EE Monsters"
		Radius 24;
		Height 56;
		Mass 500;
		Health 200;
		PainChance 256;
		MONSTER;
		+NOBLOOD
		Tag "Centaurion (Hedge)";
	}
	
	States
	{
		Spawn:
			TOPI C 10 A_Look();
			Loop;
		See:
			TOPI C 1 A_Die();
			Goto Death;
		Pain:
			TOPI C 1 A_Die();
			Goto Death;
		Death:
			TOPI C 20;
			TNT1 A 1 A_SpawnItemEX("CentaurionSgt",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION);
			TNT1 A 0 A_NoBlocking();
			Stop;
	}
}

