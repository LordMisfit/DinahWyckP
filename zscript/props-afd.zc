		Painchance 256;
		MONSTER;
		+NOBLOOD
		+DONTTHRUST
		+NORADIUSDMG
		-COUNTKILL
		Tag "Old Pipe (Prop)";
	}
	States
	{
		Spawn:
			PIPE A 10;
			Goto See;
		See:
			PIPE A 1;
			Loop;
