				let MiscItem = PlayerStatItem(player.mo.FindInventory("PlayerStatItem"));
				let globalvars = DDGlobalVariables.Get();
				bool wutthis = false;
				if (exex_ikaripointsenabled && MiscItem && globalvars && (player.mo && player.mo.CountInv("IPUseMode")))
				{
