				}
				else 
				{
					if (armor.Amount >= 124)
					{
						if (armor.Icon != TexMan.checkForTexture("AGWNI0")) armor.Icon = TexMan.checkForTexture("AGWNI0");
					}
					//armor.SavePercent = clamp(50, 0, 100) / 100;
					armor.SavePercent = clamp(50.0, 0, 100.0) / 100.0;
