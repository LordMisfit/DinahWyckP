class ArmorScrap : ArmorBonus
{
	int user_type;
	default
	{
		Inventory.Icon "DACLB0";
		Inventory.Pickupmessage "Knight-Gown Scrap!";
		Inventory.PickupSound "Scrap/Find";
		Armor.SavePercent 50;
