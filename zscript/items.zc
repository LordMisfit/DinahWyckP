//Armor
class KnightGown : SilverShield
{
	default
	{
		//$Category RPArtifacts
		Inventory.Icon "AGWNI0";
		Inventory.Pickupmessage "Knight-Gown (50% Protection)";
		Inventory.PickupSound "";
		Armor.SavePercent 50;
		Armor.SaveAmount 125;
		DamageFactor "Melee", 0.8;
		DamageFactor "Zap", 0.9;
		Scale 0.5;
		+DONTGIB
		+FLOATBOB
		Tag "Knight-Gown";
	}
	
	override bool Use (bool pickup)
	{
		int PrevArmorType;
		if (owner.CountInv("KnightGownItem")) PrevArmorType = 1;
		if (owner.CountInv("KnightGownItem2")) PrevArmorType = 2;
		if (owner.CountInv("KnightGownItem3")) PrevArmorType = 3;
		if (owner.CountInv("KnightGownItem4")) PrevArmorType = 4;
		if (dydudebug_armordisplays) Console.Printf("PrevArmorType: %d", PrevArmorType);

		int SaveAmount = GetSaveAmount();
		let armor = BasicArmor(Owner.FindInventory("BasicArmor"));

		// This should really never happen but let's be prepared for a broken inventory.
		if (armor == null)
		{
			armor = BasicArmor(Spawn("BasicArmor"));
			armor.BecomeItem ();
			Owner.AddInventory (armor);
		}
		else
		{
			// If you already have more armor than this item gives you, you can't
			// use it.
			//Console.Printf("Amt: %d, Max: %d", armor.Amount, maxa);
			int maxa = (250 + (armor.BonusCount*2));
			if (PrevArmorType == 1) maxa = (250 + (armor.BonusCount*2));
			if (PrevArmorType == 2) maxa = (500 + (armor.BonusCount*4));
			if (PrevArmorType == 3) maxa = (1000 + (armor.BonusCount*8));
			if (PrevArmorType == 4) maxa = (2000 + (armor.BonusCount*16));
			if (armor.Amount >= maxa)
			{
				return false;
			}
			// Don't use it if you're picking it up and already have some.
			if (pickup && armor.Amount > maxa)
			{
				return false;
			}
		}
		
		if (PrevArmorType <= 1)
		{
			armor.SavePercent = clamp(SavePercent, 0, 100) / 100;
			if (armor.Amount <= 0) armor.Amount = SaveAmount + armor.BonusCount;
												else armor.Amount += (SaveAmount + armor.BonusCount);
		}
		if (PrevArmorType == 2)
		{
			if (armor.Amount <= 0) armor.Amount = ((SaveAmount/2) + (armor.BonusCount/2));
												else armor.Amount += ((SaveAmount/2) + (armor.BonusCount/2));
		}
		if (PrevArmorType == 3)
		{
			if (armor.Amount <= 0) armor.Amount = ((SaveAmount/4) + (armor.BonusCount/4));
												else armor.Amount += ((SaveAmount/4) + (armor.BonusCount/4));
		}
		if (PrevArmorType == 4)
		{
			if (armor.Amount <= 0) armor.Amount = ((SaveAmount/6) + (armor.BonusCount/6));
												else armor.Amount += ((SaveAmount/6) + (armor.BonusCount/6));
		}

		if (PrevArmorType <= 1)
		{
			armor.MaxAmount = ((SaveAmount*2) + (armor.BonusCount*2));
			if (armor.Amount > armor.MaxAmount) armor.Amount = armor.MaxAmount;
		}
		if (PrevArmorType == 2)
		{
			if (armor.Amount > 500) armor.Amount = 500;
		}
		if (PrevArmorType == 3)
		{
			if (armor.Amount > 1000) armor.Amount = 1000;
		}
		if (PrevArmorType == 4)
		{
			if (armor.Amount > 2000) armor.Amount = 2000;
		}

		if (PrevArmorType <= 1)
		{
			armor.Icon = Icon;
			armor.MaxAbsorb = MaxAbsorb;
			armor.MaxFullAbsorb = MaxFullAbsorb;
			armor.ArmorType = GetClassName();
			armor.ActualSaveAmount = SaveAmount;
		}
		owner.A_StopSound(CHAN_VOICE);
		owner.A_StartSound("Dinah/marvelous",CHAN_VOICE,CHANF_DEFAULT);
		return true;
	}
	
	States
	{
		Spawn:
			AGWN A -1;
			Stop;
	}
}

