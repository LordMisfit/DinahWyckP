class BlunderShot : EEProjectile
{
	default
	{
		Tag "BlunderShot";
		Radius 3;
		Height 3;
		Speed 70;
		DamageFunction (finaldamagedealt);
		Scale 0.5;
		DamageType "Lead";
		Projectile;
		RENDERSTYLE "Normal";
		SeeSound "blundershot";
		//+NOEXTREMEDEATH
		+FORCERADIUSDMG
		ProjectileKickback 17; // 34
	}
	
	override void PostBeginPlay()
	{
		Super.PostBeginPlay();
		finaldamagedealt = (randompick(4,4,4,4,5) * random(2,16)); // 8-80
		int rad, hei;
		rad = 3;
		hei = 3;
		if (target && target.CountInv("BlunderShotUpgrade"))
		{
			if (target.CountInv("BlunderShotUpgrade") >= 3) 
			{
				finaldamagedealt = (randompick(4,5,5,5,5) * (random(11,16) + randompick(1,2,3))); // 44-95
				rad = 2;
				hei = 2;
			}
			else if (target.CountInv("BlunderShotUpgrade") >= 2) 
			{
				finaldamagedealt = (randompick(4,4,5,5,5) * (random(7,16) + randompick(0,1,2))); // 28-80
				rad = randompick(2,2,3);
				hei = randompick(2,2,3);
			}
			else if (target.CountInv("BlunderShotUpgrade") >= 1) 
			{
				finaldamagedealt = (randompick(4,4,4,5,5) * (random(4,16) + randompick(0,0,1))); // 16-85
				rad = 3;
				hei = 3;
			}
		}
		if (IPAttack) 
		{
			double type1 = 1.1;
			double type2 = 1.2;
			double type3 = 1.35;
			rad *= 0.667;
			hei *= 0.667;
			finaldamagedealt *= frandompick(type1,type1,type1,type1,type1,type1,type1,type1,type1,type2,type2,type2,type2,type3,type3);
		}
		if (score == 12) 
		{
			//Console.Printf("@!$!");
			if (target.CountInv("BlunderShotUpgrade") >= 3) finaldamagedealt *= 3;  
			else if (target.CountInv("BlunderShotUpgrade") >= 2) finaldamagedealt *= 2.55; 
			else if (target.CountInv("BlunderShotUpgrade") >= 1) finaldamagedealt *= 2.25;
			else finaldamagedealt *= 2.0;
		}
		if (rad <= 1) rad = 1;
		if (hei <= 1) hei = 1;
		A_SetSize(rad,hei);

		if (dydudebug_rangedmg) Console.Printf("damage [%s]: %d", GetClassName(), finaldamagedealt);
	}
	
	States
	{
		Spawn:
			BSHT AB 3;
			loop;
		Death:
			BBOM A 0
			{
				if (target && target.CountInv("BlunderShotUpgrade"))
				{
					if (target.CountInv("BlunderShotUpgrade") >= 3) { A_Explode(8,32,0,0,8); }
					if (target.CountInv("BlunderShotUpgrade") == 2) { A_Explode(4,16,0,0,4); }
					if (target.CountInv("BlunderShotUpgrade") == 1) { A_Explode(2,8,0,0,2); }
				}
			}
			BBOM A 4 Bright;
			BBOM BCD 4 Bright;
			stop;
		Crash:
			BBOM A 0
			{
				if (target && target.CountInv("BlunderShotUpgrade"))
				{
					if (target.CountInv("BlunderShotUpgrade") >= 3) { A_Explode(16,16,0,0,8); }
					if (target.CountInv("BlunderShotUpgrade") == 2) { A_Explode(8,8,0,0,4); }
					if (target.CountInv("BlunderShotUpgrade") == 1) { A_Explode(4,4,0,0,2); }
				}
			}
			BBOM A 4 Bright;
			BBOM BCD 4 Bright;
			stop;
		XDeath:
			TNT1 A 1 
			{
				bNOGRAVITY = true;
			}
			stop;
	}
}

