class Blunderaxe : DinahWeapon replaces Shotgun
{
	int IPATKAmmoAmt;
	int meleetimes;
	int chopcombo;
	double horzspread;
	double vertspread;
	default
	{
		//$Category EE Weapons
		Weapon.SelectionOrder 200;
		Weapon.SlotNumber 3;
		Weapon.Kickback 34;
		Weapon.AmmoType2 "BlunderAmmo";
		Weapon.AmmoGive2 6;
		//Weapon.AmmoUse2 0;
		+WEAPON.NOALERT
		+WEAPON.MELEEWEAPON
		+PUFFONACTORS
		+FLOATBOB
		Inventory.Icon "BLAXP0";
		Inventory.PickupMessage "\c[gold]Oh yes! The slow, yet trusty Blunderaxe.\c- \cx[Slot 3]\c-";
		Inventory.Pickupsound "Dinah/GetWeapon";
		DDWeapon.PickupVoice true;
		DDWeapon.PickupSound2 "misc/w_pkup";
		DDWeapon.PickupVoice2 false;
		Tag "Blunderaxe: Normal fire chops enemies in melee. Alt-fire shoots a spread of Blundershot. Chop attack occasionally inflicts a defensive debuff [up to 3 tiers] on enemies.";
		Obituary "%k used %o for target practice.";
	}
	
	action bool A_BlunderCheckComboLevel()
	{
		let MiscItem = PlayerStatItem(player.mo.FindInventory("PlayerStatItem"));
		if (invoker.chopcombo && MiscItem && MiscItem.TruePlayerLevel >= exex_axequickshotlvlrequirement) return true;
		return false;
	}
	
	action void A_BlunderRecoil(double baserecoil)
	{
		let MiscItem = PlayerStatItem(player.mo.FindInventory("PlayerStatItem"));
		int TruePlayerLevel = 0;
		if (exex_expenabled && MiscItem) TruePlayerLevel = MiscItem.PlayerLevel;
		if (CountInv("PowerStrength") || CountInv("PowerStrengthDD")) baserecoil *= 0.2;
		if (invoker.chopcombo)
		{
			if (A_BlunderCheckComboLevel()) baserecoil *= frandom(1.375,1.625);
																 else baserecoil *= frandom(1.625,1.875);
		}
		if (MiscItem)
		{
			double RecoilReduceRate = 1.00;
			RecoilReduceRate = (1.00 - (TruePlayerLevel * 0.00875));
			if (RecoilReduceRate <= 0.125) RecoilReduceRate = 0.125;
			baserecoil *= RecoilReduceRate;
		}
		A_Recoil(baserecoil);
	}
	
	action void A_BlunderShotAttack(double xyspread = 0.0, double zspread = 0.0, actor BombShots = null)
	{
		invoker.horzspread = xyspread;
		invoker.vertspread = zspread;
		if (invoker.chopcombo)
		{
			if (A_BlunderCheckComboLevel())
			{
				invoker.horzspread *= frandompick(0.667,1.334);
				invoker.vertspread *= frandompick(0.667,1.334);
			}
			else
			{
				invoker.horzspread *= frandompick(1.000,1.500);
				invoker.vertspread *= frandompick(1.000,1.500);
			}
		}
		if (random(1,2) == 1) BombShots = A_FireProjectile("BlunderShot",frandom(-invoker.horzspread,invoker.horzspread),0,0,0,0,frandom(-invoker.vertspread,invoker.vertspread));
										 else BombShots = A_FireProjectile("BlunderShot",random(-invoker.horzspread,invoker.horzspread),0,0,0,0,random(-invoker.vertspread,invoker.vertspread));
		if (BombShots)
		{
			if (random(1,128) <= (randompick(6,8,8,8,10) + (CountInv("BlunderShotUpgrade") * randompick(24,32,32,32,40)))) bombshots.bDONTREFLECT = true;
			
			if (A_BlunderCheckComboLevel())
			{
				BombShots.Score = 12;
				//Console.Printf("rn13");
			}
			if (invoker.IPAttackCheck)
			{
				EEProjectile IPCompat; 
				IPCompat = EEProjectile(BombShots);
				if (IPCompat)
				{
					IPCompat.IPattack = true;
				}
			}
		}
	}
	
	action void A_BlunderShotSmoke()
	{
		if (exex_blunderaxesmoke)
		{
			string smoketype;
			
			int smokechance = random(248,256);
			if (skill <= 0) smokechance = random(224,256);
			if (skill == 1) smokechance = random(240,256);
			if (skill == 2) smokechance = random(248,256);
			if (skill == 3) smokechance = random(252,256);
			if (skill >= 4) smokechance = random(254,256);

			double angvar = frandompick(10.0,12.5,12.5,12.5,15.0,15.0,15.0,15.0,15.0,17.5,17.5,17.5,20.0);
			if (CountInv("BlunderShotUpgrade") == 1) 
			{
				angvar *= frandom(1.125,1.25);
				smokechance *= 0.75;
			}
			if (CountInv("BlunderShotUpgrade") == 2)
			{
				angvar *= frandom(1.167,1.25);
				smokechance *= 0.45;
			}
			if (CountInv("BlunderShotUpgrade") >= 3)
			{
				angvar *= frandom(1.333,1.5);
				smokechance *= 0.1;
			}
			if (invoker.IPAttackCheck) 
			{
				angvar *= frandom(1.333,1.5);
				smokechance += int((256 - smokechance) * 0.5);
			}
			if (invoker.chopcombo) 
			{
				angvar *= frandom(1.5,1.667);
				smokechance += int((256 - smokechance) * 0.667);
			}
			if (random(1,256) <= smokechance)
			{
				if (CountInv("BlunderShotUpgrade") >= 1)
				{
					if (CountInv("BlunderShotUpgrade") >= 3)
					{
						smoketype = "BlunderSmokeSmallF";
						if (random(1,32) <= 1) smoketype = "BlunderSmokeSmallH";
						A_FireProjectile(smoketype,(angvar*0),0,0,0,0,0);
						
						smoketype = "BlunderSmokeSmall";
						if (random(1,32) <= 1) smoketype = "BlunderSmokeSmallD";
						A_FireProjectile(smoketype,angvar,0,0,0,0,0);
						A_FireProjectile(smoketype,-angvar,0,0,0,0,0);
						
						if (random(1,2) <= 1)
						{
							if (random(1,2) <= 1)
							{
								smoketype = "BlunderSmokeSmallD";
								if (random(1,8) <= 6) smoketype = "BlunderSmokeSmallF";
								A_FireProjectile(smoketype,(angvar*0),0,0,0,0,(angvar*0.5));
								A_FireProjectile(smoketype,(angvar*0),0,0,0,0,(-angvar*0.5));
							}

							smoketype = "BlunderSmokeSmall";
							if (random(1,8) <= 6) 
							{
								smoketype = "BlunderSmokeSmallD";
								if (random(1,8) <= 3) smoketype = "BlunderSmokeSmallF";
							}
							A_FireProjectile(smoketype,(angvar*0.667),0,0,0,0,0);
							A_FireProjectile(smoketype,(-angvar*0.667),0,0,0,0,0);

							smoketype = "BlunderSmokeSmall";
							if (random(1,2) <= 1) smoketype = "BlunderSmokeSmallD";
							A_FireProjectile(smoketype,angvar,0,0,0,0,(angvar*0.5));
							A_FireProjectile(smoketype,-angvar,0,0,0,0,(-angvar*0.5));

							smoketype = "BlunderSmokeSmall";
