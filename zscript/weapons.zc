	action void A_AxeChopDmg()
	{
		let MiscItem = PlayerStatItem(Player.mo.FindInventory("PlayerStatItem"));
		int PlayerSTR;
		int PlayerLevel;
		int strengthcounter;
		if (MiscItem) 
		{
			PlayerSTR = MiscItem.TruePlayerATK + MiscItem.TruePlayerLevel;
			PlayerLevel = MiscItem.TruePlayerLevel;
		}
		int TruePlayerLevel = 0;
		if (exex_expenabled && MiscItem) TruePlayerLevel = MiscItem.PlayerLevel;
		string pufftype = "BlunderPuff";
		int dmg = (random(16,20) * random(1,8));
		int range = 96;
		if (invoker.IPAttackCheck) 
		{
			dmg = (random(16,20) * random(5,8));
			pufftype = "BlunderPuffIP";
		}
		else
		{
			dmg = (random(16,20) * random(1,8));
			pufftype = "BlunderPuff";
		}
		for (strengthcounter = 0; strengthcounter < PlayerSTR; strengthcounter++)
		{
			int adddmg;
			int rngroll;
			if (random(1,96) <= (95 - (PlayerLevel * 0.0375)))
			{
				if (invoker.IPAttackCheck) adddmg = randompick(8,9,11,13,14,15);
				else adddmg = randompick(3,4,4,5,5,6);
			}
			else
			{
				if (invoker.IPAttackCheck) adddmg = randompick(16,18,19,21,23,25);
				else adddmg = randompick(6,7,8,9,10,11);
				rngroll = 1;
			}
			dmg += adddmg;
			if (dydudebug_meleedmg) Console.Printf("\c[fire]strengthcounter %d / %d [Blunderaxe]: %d (rngroll: %d)\c-", strengthcounter+1, PlayerSTR, adddmg, rngroll);
		}
		if (invoker.IPAttackCheck) 
		{
			dmg *= randompick(7,8,8,8,8,8,8,8,8,9);
			range *= 1.1875;
		}
		if (CountInv("PowerStrength") || CountInv("PowerStrengthDD")) 
		{
			dmg *= 10;
			if (MiscItem) dmg *= (1.0 + (MiscItem.TruePlayerLevel * 0.0225) + (MiscItem.TruePlayerATK * 0.0225));
		}
		else
		{
			if (MiscItem) dmg *= (1.0 + (MiscItem.TruePlayerLevel * 0.0075) + (MiscItem.TruePlayerATK * 0.0075));
		}
		
		if (dydudebug_meleedmg) Console.Printf("(BlunderPuff) dmg: %d", dmg);
		A_CustomPunch(dmg, true, 0, pufftype, range, IPCostOnHit: true);
	}

