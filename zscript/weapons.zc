	action int A_BatDamageSet(int type = 0)
	{
		let MiscItem = PlayerStatItem(Player.mo.FindInventory("PlayerStatItem"));
		int PlayerSTR;
		int PlayerLevel;
		int strengthcounter;
		if (MiscItem) 
		{
			PlayerSTR = MiscItem.TruePlayerATK + MiscItem.TruePlayerLevel;
			PlayerLevel = MiscItem.TruePlayerLevel;
		}
		int TruePlayerLevel = 0;
		if (exex_expenabled && MiscItem) TruePlayerLevel = MiscItem.PlayerLevel;

		double LevelInc;
		if (Skill <= 0) LevelInc = 9;
		if (Skill == 1) LevelInc = 10.5;
		if (Skill == 2) LevelInc = 12;
		if (Skill == 3) LevelInc = 13.5;
		if (Skill >= 4) LevelInc = 15;
		LevelInc *= 1.5;

		double dmg = random(6,8);
		if (invoker.IPAttackCheck) 
		{
			if (MiscItem)
			{
				if (TruePlayerLevel >= (LevelInc*12)) dmg *= random(11,14);
				else if (TruePlayerLevel >= (LevelInc*11)) dmg *= random(11,13);
				else if (TruePlayerLevel >= (LevelInc*10)) dmg *= random(10,13);
				else if (TruePlayerLevel >= (LevelInc*9)) dmg *= random(10,12);
				else if (TruePlayerLevel >= (LevelInc*8)) dmg *= random(9,12);
				else if (TruePlayerLevel >= (LevelInc*7)) dmg *= random(9,11);
				else if (TruePlayerLevel >= (LevelInc*6)) dmg *= random(8,11);
				else if (TruePlayerLevel >= (LevelInc*5)) dmg *= random(8,10);
				else if (TruePlayerLevel >= (LevelInc*4)) dmg *= random(7,10);
				else if (TruePlayerLevel >= (LevelInc*3)) dmg *= random(7,9);
				else if (TruePlayerLevel >= (LevelInc*2)) dmg *= random(6,9);
				else if (TruePlayerLevel >= (LevelInc*1)) dmg *= random(6,8);
				else dmg *= random(5,8);
			}
			else
			{
				dmg *= random(5,8);
			}
			dmg *= frandompick(1.25,1.5,1.5,1.5,1.5,1.5,1.75);
		}
		else
		{
			if (MiscItem)
			{
				if (TruePlayerLevel >= (LevelInc*12)) dmg *= random(7,14);
				else if (TruePlayerLevel >= (LevelInc*11)) dmg *= random(7,13);
				else if (TruePlayerLevel >= (LevelInc*10)) dmg *= random(6,13);
				else if (TruePlayerLevel >= (LevelInc*9)) dmg *= random(6,12);
				else if (TruePlayerLevel >= (LevelInc*8)) dmg *= random(5,12);
				else if (TruePlayerLevel >= (LevelInc*7)) dmg *= random(5,11);
				else if (TruePlayerLevel >= (LevelInc*6)) dmg *= random(4,11);
				else if (TruePlayerLevel >= (LevelInc*5)) dmg *= random(4,10);
				else if (TruePlayerLevel >= (LevelInc*4)) dmg *= random(3,10);
				else if (TruePlayerLevel >= (LevelInc*3)) dmg *= random(3,9);
				else if (TruePlayerLevel >= (LevelInc*2)) dmg *= random(2,9);
				else if (TruePlayerLevel >= (LevelInc*1)) dmg *= random(2,8);
				else dmg *= random(1,8);
			}
			else
			{
				dmg *= random(1,8);
			}
		}
		for (strengthcounter = 0; strengthcounter < PlayerSTR; strengthcounter++)
		{
			int adddmg;
			int rngroll;
			if (random(1,128) <= (127 - (PlayerLevel * 0.025)))
			{
				if (invoker.IPAttackCheck) adddmg = randompick(2,3,4,5,5);
				else adddmg = randompick(1,2,2,2,3,3);
			}
			else
			{
				if (invoker.IPAttackCheck) adddmg = randompick(3,4,5,6,7);
				else adddmg = randompick(2,3,4,4,5,6);
				rngroll = 1;
			}
			dmg += adddmg;
			if (dydudebug_meleedmg) Console.Printf("\c[fire]strengthcounter %d / %d [Cricket Bat]: %d (rngroll: %d)\c-", strengthcounter+1, PlayerSTR, adddmg, rngroll);
		}
		if (CountInv("PowerStrength") || CountInv("PowerStrengthDD")) 
		{
			dmg *= 10;
			if (MiscItem) dmg *= (1.0 + (MiscItem.TruePlayerLevel * 0.0225) + (MiscItem.TruePlayerATK * 0.0225));
		}
		else
		{
			if (MiscItem) dmg *= (1.0 + (MiscItem.TruePlayerLevel * 0.0075) + (MiscItem.TruePlayerATK * 0.0075));
		}
		if (type == 1) // type 1 = Spin attack
		{
			if (Skill <= 0) LevelInc = 18;
			if (Skill == 1) LevelInc = 21;
			if (Skill == 2) LevelInc = 24;
			if (Skill == 3) LevelInc = 27;
			if (Skill >= 4) LevelInc = 30;
			LevelInc *= 1.5;

			if (MiscItem)
			{
				if (TruePlayerLevel >= (LevelInc*8)) dmg /= frandom(2.0,4.0);
				else if (TruePlayerLevel >= (LevelInc*7)) dmg /= frandom(2.5,4.0);
				else if (TruePlayerLevel >= (LevelInc*6)) dmg /= frandom(2.5,4.5);
				else if (TruePlayerLevel >= (LevelInc*5)) dmg /= frandom(3.0,4.5);
				else if (TruePlayerLevel >= (LevelInc*4)) dmg /= frandom(3.0,5.0);
				else if (TruePlayerLevel >= (LevelInc*3)) dmg /= frandom(3.5,5.0);
				else if (TruePlayerLevel >= (LevelInc*2)) dmg /= frandom(3.5,5.5);
				else if (TruePlayerLevel >= (LevelInc*1)) dmg /= frandom(4,5.5);
				else dmg /= frandom(4.0,6.0);
				if (invoker.IPAttackCheck) dmg *= frandompick(1.25,1.5,1.75);
			}
			else
			{
				dmg /= frandom(4.0,6.0); 
				if (invoker.IPAttackCheck) dmg *= frandompick(1.05,1.10,1.15);
			}
		}
		dmg = int(dmg);
		if (dydudebug_meleedmg) Console.Printf("(BatPuff) dmg: %d", dmg);

		return dmg;
	}
	
