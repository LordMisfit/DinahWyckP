				else if (invoker.recordalttimer > 2160) recordinterval = 38;
				else if (invoker.recordalttimer > 2070) recordinterval = 40;
				else if (invoker.recordalttimer > 1980) recordinterval = 45;
				else if (invoker.recordalttimer > 1890) recordinterval = 50;
				else if (invoker.recordalttimer > 1800) recordinterval = 55;
				else if (invoker.recordalttimer > 1620) recordinterval = 60;
				else if (invoker.recordalttimer > 1530) recordinterval = 65;
				else if (invoker.recordalttimer > 1440) recordinterval = 70;
				else if (invoker.recordalttimer > 1350) recordinterval = 75;
				else if (invoker.recordalttimer > 1260) recordinterval = 80;
				else if (invoker.recordalttimer > 990) recordinterval = 85;
				else if (invoker.recordalttimer > 540) recordinterval = 95;
				else if (invoker.recordalttimer > 90) recordinterval = 105;
				A_GrimophoneAltDecModTimer(recordinterval,recintervalIPmult);
				if (invoker.IPAttackCheck)
				{
					if (invoker.modtimer < 15) invoker.modtimer = 15;
				}
				else
				{
					if (invoker.modtimer < 10) invoker.modtimer = 10;
				}
				if (dydudebug_grimophonealtfire) Console.Printf("\c[brick]modtimer: %d\c-, recordalttimer / recordinterval: %d / %d, \c[yellow]Modulus: %d\c-", invoker.modtimer, invoker.recordalttimer, recordinterval, (invoker.recordalttimer % invoker.modtimer));

				A_Explode(invoker.dmg,invoker.dist,invoker.altflags,1,invoker.fulldist,0,0,"","Beats");
				A_RadiusThrust((invoker.dmg*-1),invoker.dist,invoker.altflags2,(invoker.fulldist*-1));
				int MusicCritChance = 1;
				if (invoker.IPAttackCheck) MusicCritChance = 5;
				if (random(1,64) == MusicCritChance)
				{
					A_Explode((invoker.dmg*5),invoker.dist,XF_NOTMISSILE|XF_NOSPLASH,1,invoker.fulldist,0,0,"","Beats");
					A_RadiusThrust((invoker.dmg*-5),invoker.dist,RTF_NOTMISSILE|RTF_NOIMPACTDAMAGE,(invoker.fulldist*-1));
				}
				
				if (!(player.bINVULNERABLE ||
					 player.CountInv("PowerInvulnerable") || player.CountInv("PowerInvulnDD") || 
					 player.player.cheats & CF_GODMODE2 || player.player.cheats & CF_GODMODE))
				{
					if (MiscItem) 
					{
						if (player.CountInv("GrimophoneUpgrade"))
						{
							if (player.CountInv("GrimophoneUpgrade") >= 3)
							{
								if (invoker.IPAttackCheck) MiscItem.GrimophoneDebuffTimer += 5;
																			else MiscItem.GrimophoneDebuffTimer += 6;
							}
							else
							if (player.CountInv("GrimophoneUpgrade") == 2)
							{
								if (invoker.IPAttackCheck) MiscItem.GrimophoneDebuffTimer += 4;
																			else MiscItem.GrimophoneDebuffTimer += 5;
							}
							else
							if (player.CountInv("GrimophoneUpgrade") == 1)
							{
								if (invoker.IPAttackCheck) MiscItem.GrimophoneDebuffTimer += 3;
																			else MiscItem.GrimophoneDebuffTimer += 4;
							}
						}
						else
						{
							if (invoker.IPAttackCheck) MiscItem.GrimophoneDebuffTimer += 2;
																		else MiscItem.GrimophoneDebuffTimer += 3;
						}
					}
				}
			}
			RPLY C 0 
			{
				actor player = players[0].mo;
				A_GrimophoneAltOn();
				
				int energybitcost;
				if (invoker.IPAttackCheck) energybitcost = 5;
															else energybitcost = 10;
				if (player.CountInv("GrimophoneUpgrade") <= 0) 
				{
					energybitcost *= 1.0;
					if (random(1,32) <= 1) energybitcost *= 2;
				}
				if (player.CountInv("GrimophoneUpgrade") == 1)
				{
					energybitcost *= frandom(0.875,0.925);
					if (random(1,28) <= 1) energybitcost *= 2;
				}
				if (player.CountInv("GrimophoneUpgrade") == 2)
				{
					energybitcost *= frandom(0.75,0.85);
					if (random(1,24) <= 1) energybitcost *= 2;
				}
				if (player.CountInv("GrimophoneUpgrade") >= 3) 
				{
					energybitcost *= frandom(0.5,0.7);
					if (random(1,16) <= 1) energybitcost *= 2;
				}
				invoker.recordenergybits -= energybitcost;
				
				while (invoker.recordenergybits <= 0)
				{
					invoker.recordenergy--;
					invoker.recordenergybits += 10;
				}
				if (dydudebug_grimophonealtfire) Console.Printf("\c[blue]recordenergy: %d\c-, \c[sapphire]recordenergybits: %d / 20\c-, energybitcost: %d", invoker.recordenergy, invoker.recordenergybits, energybitcost);
				
				invoker.A_CheckIPModeOnUse();
				
				if (invoker.recordenergy <= 0)
				{
					if (CountInv("MusicAmmo") <= 0)
					{
						A_StopSound(65);
						return resolvestate("Ready");
					}
					else
					{
						A_Refire("AltFire2");
					}
					return resolvestate(null);
				}
				else
				{
					A_Refire("AltFire2");
				}
				return resolvestate(null);
			}
			RPLY A 0 
			{
				A_GrimophoneAltOn();
				A_StopSound(65);
			}
			Goto Ready;
	}
}

