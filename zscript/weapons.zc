class AppleJack : DinahWeapon replaces Chaingun
{
	default
	{
		//$Category EE Weapons
		Weapon.SelectionOrder 100;
		Weapon.SlotNumber 4;
		Weapon.Kickback 60;
		Weapon.AmmoType2 "JackAmmo";
		Weapon.AmmoGive2 12;
		//Weapon.AmmoUse2 1;
		+WEAPON.NOALERT
		+WEAPON.MELEEWEAPON
		+PUFFONACTORS
		+FLOATBOB
		Inventory.Icon "AJAKP0";
		Inventory.Pickupsound "Dinah/GetWeapon";
		DDWeapon.PickupVoice true;
		DDWeapon.PickupSound2 "misc/w_pkup";
		DDWeapon.PickupVoice2 false;
		Inventory.PickupMessage "\c[gold]Nice, I found an Apple Jack! Not as defense based as the Cricket Bat, but nice for offense!\c- \cx[Slot 4]\c-";
		Tag "Apple Jack: Normal attack is a melee swing that hit twice. Alt attack is a ranged toss of an aggro-apple that can usually block incoming projectiles. Swings can also affect enemy projectiles.";
		Obituary "%k kept the doctor away from %o.";
	}
	
	action void A_AppleMeleeAtk()
	{
		actor player = players[0].mo;
		let MiscItem = PlayerStatItem(player.FindInventory("PlayerStatItem"));
		int TruePlayerLevel = 0;
		int PlayerSTR;
		int PlayerLevel;
		int strengthcounter;
		if (MiscItem) 
		{
			PlayerSTR = MiscItem.TruePlayerATK + MiscItem.TruePlayerLevel;
			PlayerLevel = MiscItem.TruePlayerLevel;
		}
		if (exex_expenabled && MiscItem) TruePlayerLevel = MiscItem.PlayerLevel;
		
		double decenthealthfactor = 0.125;
		double lowhealthfactor = 0.375;
		double crisishealthfactor = 1.75;

		int dmg = (random(6,8) * random(1,8));
		int atkrange = 96;
		if (invoker.IPAttackCheck) 
		{
			dmg = (random(6,8) * random(5,8));
			dmg *= frandompick(1.25,1.5,1.5,1.5,1.5,1.5,1.75);
		}
		if (MiscItem)
		{
			if (TruePlayerLevel >= exex_jackcrisislvlrequirement && exex_jackcrisislvlrequirement > 0)
			{
				decenthealthfactor *= (1.0 + ((TruePlayerLevel / exex_jackcrisislvlrequirement) * 0.5));
				lowhealthfactor *= (1.0 + ((TruePlayerLevel / exex_jackcrisislvlrequirement) * 0.667));
				crisishealthfactor *= (1.0 + ((TruePlayerLevel / exex_jackcrisislvlrequirement) * 1.0));
				if (player.CountInv("DecentHealth")) dmg *= (1.0 + decenthealthfactor);
				if (player.CountInv("LowHealth")) dmg *= (1.0 + lowhealthfactor);
				if (player.CountInv("CriticalHealth")) dmg *= (1.0 + crisishealthfactor);
				if (dydudebug_meleedmg) Console.Printf("decenthealthfactor: %.2f, lowhealthfactor: %.2f, crisishealthfactor: %.2f", decenthealthfactor, lowhealthfactor, crisishealthfactor);
			}
		}
		for (strengthcounter = 0; strengthcounter < PlayerSTR; strengthcounter++)
		{
			int adddmg;
			int rngroll;
			if (random(1,128) <= (127 - (PlayerLevel * 0.025)))
			{
				if (invoker.IPAttackCheck) adddmg = randompick(2,2,3,3,4,4);
				else adddmg = randompick(1,1,2,2,2,3);
			}
			else
			{
				if (invoker.IPAttackCheck) adddmg = randompick(3,4,5,5,6,6);
				else adddmg = randompick(2,3,3,4,4,5);
				rngroll = 1;
			}
			dmg += adddmg;
			if (dydudebug_meleedmg) Console.Printf("\c[fire]strengthcounter %d / %d [AppleJack]: %d (rngroll: %d)\c-", strengthcounter+1, PlayerSTR, adddmg, rngroll);
		}
		if (CountInv("PowerStrength") || CountInv("PowerStrengthDD")) 
		{
			dmg *= 10;
			if (MiscItem) dmg *= (1.0 + (MiscItem.TruePlayerLevel * 0.0225) + (MiscItem.TruePlayerATK * 0.0225));
		}
		else
		{
			if (MiscItem) dmg *= (1.0 + (MiscItem.TruePlayerLevel * 0.0075) + (MiscItem.TruePlayerATK * 0.0075));
		}
		if (dydudebug_meleedmg) Console.Printf("(ApplePuff) dmg: %d", dmg);

		string pufftype = "ApplePuff";
		double shotangle = 0;
		double shotangleend = 0;
		double shotangleinc = 0;
		double origangle = angle;
		int maxshots = 1;
		if (invoker.IPAttackCheck) // >.> <.<
		{
			pufftype = "ApplePuffIP";
			if (random(1,32) <= 1)
			{
				shotangle = -33.75;
				shotangleend = 33.75;
				shotangleinc = 11.25;
				maxshots = 7;
				atkrange *= 1.5;
			}
			else
			{
				shotangle = -22.5;
				shotangleend = 22.5;
				shotangleinc = 11.25;
				maxshots = 5;
				atkrange *= 1.334;
			}
		}
		if (invoker.IPAttackCheck) A_SetAngle(angle+shotangle);

		for (int i = 0; i < maxshots; i++)
		{
			if (i && invoker.IPAttackCheck) A_SetAngle(angle+shotangleinc);
			A_CustomPunch(dmg, true, 0, pufftype, atkrange, IPCostOnHit: true);
			if (CountInv("PowerStrength") || CountInv("PowerStrengthDD"))
			{
				A_AppleJackSwingShield(1);
			}
			else
			{
				A_AppleJackSwingShield(0);
			}
		}
		if (invoker.IPAttackCheck) A_SetAngle(origangle);
	}
	
	action void A_AppleJackAltAtk3Tics()
	{
		let MiscItem = PlayerStatItem(player.mo.FindInventory("PlayerStatItem"));
		int TruePlayerLevel = 0;
		if (exex_expenabled && MiscItem) TruePlayerLevel = MiscItem.PlayerLevel;
		
		if (CountInv("PowerStrength") || CountInv("PowerStrengthDD")) 
		{
			if (TruePlayerLevel)
			{
