						if (success) A_JackShotShieldSetup(BombShots,0.0125);
						[success, BombShots] = A_SpawnItemEx("JackShotShieldPart",-1.5,0,4.5,0.0,0.0,0.0,0,SXF_SETTARGET|SXF_NOCHECKPOSITION);
						if (success) A_JackShotShieldSetup(BombShots,0.0125);
