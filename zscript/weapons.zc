class FCandlePickup : ExExInvBarItem 
{
	default
	{
		//$Category EE Artifacts
		+INVENTORY.INVBAR
		+INVENTORY.HUBPOWER
		+INVENTORY.PERSISTENTPOWER
		+INVENTORY.KEEPDEPLETED
		+DONTGIB
		+COUNTITEM
		+FLOATBOB
		Inventory.PickupSound "Dinah/marvelous";
		CustomInventoryExt.PickUpVoice true;
		Scale 0.925;
		Inventory.Amount 1;
		Inventory.MaxAmount 2; // 2
		Inventory.InterHubAmount 2; // 2
		Inventory.PickupMessage "\c[gold]'Holy crap, is this a VulcanCandle for Delila's Fosnian CandleVulcan? Nice. >:D'\c-";
		Inventory.Icon "FCNAE0";
		Tag "Fosnian VulcanCandle: Fully replenishes the Fosnian CandleVulcan, but only when it is depleted and if it's been picked up previously.";
	}
	
	States
	{
		Spawn:
			FCNA E 1
			{
				actor player = players[0].mo;
				let MiscItem = PlayerStatItem(Player.FindInventory("PlayerStatItem"));
				if (!player.CountInv("FosnianCandle"))
				{
					A_SpawnItemEx("FosnianCandle",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_TRANSFERSCALE|SXF_TRANSFERALPHA|SXF_TRANSFERRENDERSTYLE);
					Destroy();
				}
			}
			Loop;
		Use:
			TNT1 A 0 
			{
				actor player = players[0].mo;
				let MiscItem = PlayerStatItem(Player.FindInventory("PlayerStatItem"));
				if (player.CountInv("FosnianCandle"))
				{
					if (player.CountInv("FCandleAmmo") <= 0)
					{
						player.A_SetInventory("FosnianCandle",1);
						player.A_GiveInventory("FCandleAmmo",9999999);
						player.A_StopSound(CHAN_VOICE);
						player.A_StartSound("Dinah/Giggle",CHAN_VOICE);
						player.A_TakeInventory("FCandlePickup",1);
					}
					else
					{
						A_StartSound("inv_fail",84,CHANF_DEFAULT,1.0,ATTN_NORM);
						A_PrintBold(String.Format("\cgCan't use right now! You have ammo in your Fosnian VulcanCandle right now!"), frandom(0.685,0.8));
					}
				}
				else
				{
					A_StartSound("inv_fail",84,CHANF_DEFAULT,1.0,ATTN_NORM);
					A_PrintBold(String.Format("\cgCan't use right now! You don't have a Fosnian VulcanCandle right now!"), frandom(0.685,0.8));
				}
			}
			fail;
	}
}
