					exprad += (TruePlayerLevel * 0.256);
					exprad *= (1.0 + (TruePlayerLevel * 0.25));
					exprad2 += (TruePlayerLevel * 0.192);
					exprad2 *= (1.0 + (TruePlayerLevel * 0.25));
				}
				A_Explode(expdmg,exprad,false,true,exprad);
			}
			Stop;
	}
}

