//FOSNIAN CANDLE - FROM CARNIVAL OF SOULS V:
class FosnianCandle : DinahWeapon
{
	int shottype;
	default
	{
		//$Category RPWeapons
		Weapon.SelectionOrder 800;
		Weapon.AmmoType1 "FCandleAmmo";
		Weapon.AmmoType2 "FCandleAmmo";
		Weapon.AmmoUse1 1;
		Weapon.AmmoUse2 1;
		Weapon.AmmoGive 99999999;
		Weapon.SlotNumber 8;
		Weapon.KickBack 125;
		+FLOATBOB;
		inventory.pickupsound "Dinah/marvelous";
		DDWeapon.PickupVoice true;
		DDWeapon.PickupSound2 "misc/w_pkup";
		DDWeapon.PickupVoice2 false;
		Inventory.Pickupmessage "\c[gold]Ohohoho...! Delila's Fosnian CandleVulcan! Heck yeah! This thing would easily decimate Napoleon's entire army!\c- \cx[Slot 0]\c-";
		Tag "Fosnian CandleVulcan: Fires a fast and perfectly accurate 3-shot explosive burst akin to a vulcan cannon. <ALT-FIRE> fires a speedier, less accurate trio of shots. <USER4> Swaps the damage type between FIRE and ICE. Light though it might be, it has very strong recoil, be weary on ledges.";
	}
	
	action void A_FCandleShot(double angleerr = 0.0, double pitcherr = 0.0, double recoilerr = 0.0)
	{
		bool debugrecoil = false;
		let player = self.player;
		let MiscItem = PlayerStatItem(player.mo.FindInventory("PlayerStatItem"));
		int TruePlayerLevel = 0;
		if (exex_expenabled && MiscItem) TruePlayerLevel = MiscItem.PlayerLevel;

		double baserecoil = 1.0;
		double crouchrefa;
		if (player.onground) 
		{
			crouchrefa = ((1.0 - player.crouchfactor) * 1.5);
		}
		else 
		{
			crouchrefa = ((1.0 - player.crouchfactor) * 1.125);
		}
		baserecoil *= 1.0 - crouchrefa;
		
		double sndpitch = 1.0;
		double sndvol = 1.0;
		A_StartSound("weapons/fcandlepopping",random(501,510),CHANF_DEFAULT,(sndvol*1.5),ATTN_NORM,sndpitch);
		if (invoker.IPAttackCheck)
		{
			string atktype = "CandleShotIP";
			if (invoker.shottype >= 1) atktype = "CandleShotIcyIP";
			A_StartSound("weapons/fcandlefire",CHAN_WEAPON,CHANF_DEFAULT,sndvol,ATTN_NORM,sndpitch);
			A_StartSound("weapons/fcandlepopping",random(511,520),CHANF_DEFAULT,(sndvol*1.5),ATTN_NORM,sndpitch);
			if (random(1,10) <= 7) 
			{
				sndpitch = frandom(0.75,0.875);
				sndvol = 1.2;
				A_StopSound(CHAN_7);
				A_StartSound("weapons/fcandlefire",CHAN_7,CHANF_DEFAULT,sndvol,ATTN_NORM,sndpitch);
				A_StartSound("weapons/fcandlepopping",random(521,530),CHANF_DEFAULT,(sndvol*1.5),ATTN_NORM,sndpitch);
				if (random(1,10) <= 5) 
				{
					sndpitch = frandom(0.625,0.75);
					sndvol = 1.4;
					A_StopSound(8);
					A_StartSound("weapons/fcandlefire",8,CHANF_DEFAULT,sndvol,ATTN_NORM,sndpitch);
					A_StartSound("weapons/fcandlepopping",random(531,540),CHANF_DEFAULT,(sndvol*1.5),ATTN_NORM,sndpitch);
				}
			}
			A_FireProjectile(atktype, frandom(-angleerr,angleerr), true, 0, 0, 0, frandom(-pitcherr,pitcherr));
			A_Quake(random(4,5),random(2,3),0,32,0);
			baserecoil *= (random(45,55) * 0.01);
		}
		else 
		{
			string atktype = "CandleShot";
			if (invoker.shottype >= 1) atktype = "CandleShotIcy";
			A_StartSound("weapons/fcandlefire",CHAN_WEAPON);
			A_FireProjectile(atktype, frandom(-angleerr,angleerr), true, 0, 0, 0, frandom(-pitcherr,pitcherr));
			A_Quake(9,5,0,64,0);
		}
		baserecoil *= (1.0 - (TruePlayerLevel * 0.025));
		if (baserecoil < 0.0) baserecoil = 0.0;
		
		double truerecoil = baserecoil + (recoilerr * baserecoil);
		if (debugrecoil) Console.Printf("\c[fire]Recoil: %.8f,\c- Base: %.8f, Error: %.8f, crouchrefa, %.8f, \c[sapphire]player.crouchfactor: %.8f", truerecoil, baserecoil, recoilerr, crouchrefa, player.crouchfactor);
		A_Recoil(truerecoil);
	}
	
	States
	{
		Spawn:
			FCND P 1
			{
				actor player = players[0].mo;
				let MiscItem = PlayerStatItem(Player.FindInventory("PlayerStatItem"));
				if (player.CountInv("FosnianCandle") > 0)
				{
					A_SpawnItemEx("FCandlePickup",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_TRANSFERSCALE|SXF_TRANSFERALPHA|SXF_TRANSFERRENDERSTYLE);
					Destroy();
				}
			}
			Loop;
		Ready:
			FCND A 1 A_WeaponReady(WRF_ALLOWUSER4);
			Loop;
		Deselect:
			FCND A 1 A_Lower(12);
			Loop;
		Select:
			FCND A 1 A_Raise(12);
			Loop;
		Fire:
			FCND B 0 Bright
			{
				if (invoker.shottype >= 1) return resolvestate("FireIce");
				A_CheckIPState1();
				A_ConfirmIPAttack(201,0,3,11,true);
				return resolvestate(null);
			}
			FCND B 2 Bright 
			{
				if (invoker.IPAttackCheck)
				{
					if (random(1,4) <= 1) A_SetTics(random(1,2));
				}
				A_FCandleShot(0,0,0);
			}
			FCND A 1
			{
				if (invoker.IPAttackCheck)
				{
					if (random(1,5) <= 4) A_SetTics(0);
				}
			}
			FCND C 2 Bright
			{
				if (invoker.IPAttackCheck)
				{
					if (random(1,4) <= 1) A_SetTics(random(1,2));
				}
				A_FCandleShot(0,0,0);
			}
			FCND A 1
			{
				if (invoker.IPAttackCheck)
				{
					if (random(1,5) <= 4) A_SetTics(0);
				}
			}
			FCND D 2 Bright
			{
				if (invoker.IPAttackCheck)
				{
					if (random(1,4) <= 1) A_SetTics(random(1,2));
				}
				A_FCandleShot(0,0,0);
			}
			FCND A 1 
			{
				if (invoker.IPAttackCheck)
				{
					if (random(1,5) <= 4) A_SetTics(0);
				}
				invoker.A_CheckIPModeOnUse();
				A_ReFire();
			}
			goto Ready;
		FireIce:
			FCND E 0 Bright
			{
				A_CheckIPState1();
				A_ConfirmIPAttack(201,0,3,11,true);
				return resolvestate(null);
			}
			FCND E 2 Bright 
			{
				if (invoker.IPAttackCheck)
				{
					if (random(1,4) <= 1) A_SetTics(random(1,2));
				}
				A_FCandleShot(0,0,0);
			}
			FCND A 1
			{
				if (invoker.IPAttackCheck)
				{
					if (random(1,5) <= 4) A_SetTics(0);
				}
			}
			FCND F 2 Bright
