			}
			Goto Spawn;
		Missile:
			DINA FG 6
			{
			}
			Goto Spawn;
		Melee:
			DINA FGF 4
			{
			}
			Goto Spawn;
		BatGuard:
			DINA LL 1
			{
			}
			DINA L 0
			{
			}
			Goto Spawn;
		ShieldGuard:
			DINA LL 2
			{
			}
			DINA L 0
			{
			}
			Goto Spawn;
		SpinAttackBatPre:
