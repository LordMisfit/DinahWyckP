					bNOBLOOD = true;
					bNOBLOODDECALS = true;
				}
			}
		}
		if (dydudebug_playerdamagemobj) Console.Printf("(A_CheckNeverBleed [player], type: %d, damage: %d)", type, damage);
	}
	
