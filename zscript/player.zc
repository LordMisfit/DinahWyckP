				A_SetInventory("PKicking",0);
			}
			Goto Spawn;
		Death:
			DINA H 0 A_APPBreatheReset();
			DINA H 9 A_PlayerScream;
			DINA I 9 A_NoBlocking();
			DINA JK 9;
			DINA K -1;
			Stop;
		XDeath:
			DINA H 0 A_APPBreatheReset();
			DINA H 9 A_PlayerScream;
			DINA I 9 A_NoBlocking();
			DINA JK 9;
			DINA K -1;
			Stop;
		AltSkinDeath:
			DINA H 0 A_APPBreatheReset();
			DINA H 9 A_PlayerScream;
			DINA I 9 A_NoBlocking();
			DINA JK 9;
			DINA K -1;
			Stop;
		AltSkinXDeath:
			DINA H 0 A_APPBreatheReset();
			DINA H 9 A_PlayerScream;
			DINA I 9 A_NoBlocking();
			DINA JK 9;
			DINA K -1;
			Stop;
		Burn:
			goto Death;
		Ice:
			goto Death;
  }
}

