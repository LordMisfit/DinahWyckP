						if (item is 'Weapon' || item is 'BasicArmor' || item is 'HexenArmor' || item is 'Ammo' || item is 'VacAmmoCounter' ||
								item is 'Backpack' || item is 'BlunderShotUpgrade' || item is 'GrimophoneUpgrade' || item is 'SigilPieces' || item is 'SigilModeType')
						{
