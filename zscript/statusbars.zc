			VisiblePlayerLevel = MiscItem.PlayerLevel;
			VisiblePlayerATK = MiscItem.PlayerATK;
			VisiblePlayerDEF = MiscItem.PlayerDEF;
			VisiblePlayerSPD = MiscItem.PlayerSPD;
		}

		if (screenblocks < 12)
		{
			vector2 FontScale = (1.25, 1.25);
			int FontLineSpace = 4;
			int FontWrapWidth = -1;
			double FontAlpha = 0.9;
			int HPShakeFormX = 0;
			int HPShakeFormY = 0;
			int APShakeFormX = 0;
			int APShakeFormY = 0;
			int IPShakeFormX = 0;
