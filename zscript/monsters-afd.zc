			TNT1 AAAAAAAA 0 A_Wander();
			TNT1 A 0 A_UnsetSolid();
			TNT1 AAAAAAAA 1 A_Wander();
			TNT1 A 4 A_SetSolid();
			TNT1 A 0 A_SpawnItemEx("TeleportFog");
			Goto See;
		Escape2:
