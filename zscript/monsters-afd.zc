// AFD MONSTERS AND THINGS 
//MEISTER KRAG
class Teacher_Krag : DDOrigBoss
{
	default
	{
		//$Category RPTeachers
		Health 1500;
		Radius 32;
		Height 80;
		Speed 18;
		PainChance 128;
		Painchance "Suck", 64;
		Painchance "Breakfast", 0;
		Painchance "Beats", 0;
		DamageFactor "Bonk", 0.5;
		DamageFactor "Fire", 0.5;
		DamageFactor "Beats", 0.25;
		DamageFactor "Chop", 2;
		DamageFactor "Breakfast", 0;
		DamageFactor "suck", 0;
		MeleeThreshold 208;
		MeleeRange 80;
		Mass 5000;
		Scale 1.5;
		MONSTER;
		+FLOORCLIP
		+NOBLOOD
		+NOINFIGHTING
		+DONTHARMCLASS
		+NORADIUSDMG
		SeeSound "krag/sight";
		PainSound "krag/pain";
		DeathSound "krag/death";
		ActiveSound "krag/active";
		MeleeSound "mallethit";
		Obituary "%o was flunked by Meister Krag.";
		Tag "Meister Krag";
	}
	
	States
	{
		Spawn:
			MKRG J 10 A_Look();
			Loop;
		See:
			MKRG A 0 A_JumpIfHealthLower(1000,"SeeFast");
			MKRG AABBCCDD 3 A_Chase();
			Loop;
		SeeFast:
			MKRG AABBCCDD 2 A_Chase();
			Loop;
		Melee:
			MKRG E 0; //ACS_Execute(102,0,0,0,0);
			MKRG E 0 A_JumpIfHealthLower(1000,"MeleeFast");
			MKRG E 0 A_StartSound("krag/throw",CHAN_VOICE);
			MKRG EF 4 A_FaceTarget();
			MKRG G 6 A_CustomMeleeAttack (random(35,50), "mallethit", "malletswing","bonk", 1);
			MKRG HIJ 6; 
			Goto See;
		MeleeFast:
			MKRG E 0 A_StartSound("krag/throw",CHAN_VOICE);
			MKRG EF 3 A_FaceTarget();
			MKRG G 4 A_CustomMeleeAttack (random(45,65), "mallethit", "malletswing","bonk", 1);
			MKRG HIJ 4;
			Goto See;
		Missile:
			MKRG K 0; //ACS_Execute(102,0,0,0,0);
			MKRG K 0 A_JumpIfHealthLower(800,"MissileFast");
			MKRG K 0 A_StartSound("krag/throw",CHAN_VOICE);
			MKRG KL 4 A_FaceTarget();
			MKRG M 6 A_SpawnProjectile("KragBall",40,0,random(-3,3));
			MKRG N 6;
			Goto See;
		MissileFast:
			MKRG K 0 A_StartSound("krag/throw",CHAN_VOICE);
			MKRG K 0; //ACS_Execute(102,0,0,0,0);
			MKRG KL 3 A_FaceTarget();
			MKRG M 4 A_SpawnProjectile("KragBall",40,0,random(-3,3));
			MKRG N 4;
			MKRG Q 0 A_StartSound("krag/throw",CHAN_VOICE);
			MKRG QR 3 A_FaceTarget();
			MKRG S 4 A_SpawnProjectile("KragBall",40,0,random(-3,3));
			MKRG T 4;
			Goto See;
		Pain:
			MKRG F 6 A_Pain();
			MKRG F 3; //ACS_Execute(102,0,0,0,0);
			Goto See;
		Death:
			MKRG E 5 A_Scream();
			MKRG OP 5 bright;
			TBUM A 0 A_StartSound("teacher/death");
			TBUM ABCDEFGH 5 bright;
			TBUM I 5 bright A_NoBlocking();
			Stop;
	}
}

class KragClone : Teacher_Krag
{
	default
	{
		health 1500;
		Speed 18;
		Obituary "%o got flunked by a clone of Meister Krag.";
		Tag "Meister Krag (clone)";
	}
}

class KragBall : DDProjectile
{
	default
	{
		Radius 8;
		Height 6;
		Speed 15;
		Damage 10;
		Scale 0.5;
		Projectile;
		Gravity 0.125;
		-NOGRAVITY
		DontHurtShooter;
		BounceType "Hexen";
		Bouncefactor 1.4;
		Bouncecount 8;
		BounceSound "axe/hit";
		SeeSound "axe/throw";
		DeathSound "axe/hit";
	}
	
	States
	{
		Spawn:
			KRBL ABCD 4;
			Loop;
		Death:
			KRBL A 4 Bright A_Explode(10,96);
			KRBL E 10 Bright;
			Stop;
	}
}

