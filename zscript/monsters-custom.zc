			}
			loop;

		Melee: 
		Missile:
			TNT1 A 0
			{
				A_CheckIfTargetDead("See");
			}
			TNT1 A 0
			{
				if (!(CountInv("ChaingunCooldown")))
				{
					if (exex_monsterlevelenabledflags) { user_shotsfiredmax += random((30 + (CountInv("MonsterLevel") / A_CheckMonsterLevelDiv(10,true,self))),(90 + (CountInv("MonsterLevel") / 3.34))); }
					else { user_shotsfiredmax = random(30,90); }
					A_DynamicMusic();
				}
				else
				{
					return resolvestate("See");
				}
				return resolvestate(null);
			}
			CRG2 H 9
			{
				A_FaceTarget();
				user_shotaccuracy = 15.75; // 11.25; // 22.5;
				user_shotzaccuracy = 4.5; // 0.0;
				if (exex_monsterlevelenabledflags && A_CheckMonsterLevel(100,2,true,self)) { user_shotaccuracy = user_shotaccuracy * 5 / 8; user_shotzaccuracy = user_shotzaccuracy * 3 / 8; }
				else if (exex_monsterlevelenabledflags && A_CheckMonsterLevel(80,2,true,self)) { user_shotaccuracy = user_shotaccuracy * 6 / 8; user_shotzaccuracy = user_shotzaccuracy * 4 / 8; }
				else if (exex_monsterlevelenabledflags && A_CheckMonsterLevel(60,2,true,self)) { user_shotaccuracy = user_shotaccuracy * 6 / 8; user_shotzaccuracy = user_shotzaccuracy * 5 / 8; }
				else if (exex_monsterlevelenabledflags && A_CheckMonsterLevel(40,2,true,self)) { user_shotaccuracy = user_shotaccuracy * 6 / 8; user_shotzaccuracy = user_shotzaccuracy * 6 / 8; }
				else if (exex_monsterlevelenabledflags && A_CheckMonsterLevel(20,2,true,self)) { user_shotaccuracy = user_shotaccuracy * 7 / 8; user_shotzaccuracy = user_shotzaccuracy * 7 / 8; }
				user_shotaccuracy1 = user_shotaccuracy * -0.5;
				user_shotaccuracy2 = user_shotaccuracy * 0.5;
				user_shotzaccuracy1 = user_shotzaccuracy * -0.5;
				user_shotzaccuracy2 = user_shotzaccuracy * 0.5;
			}
