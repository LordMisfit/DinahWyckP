				else if (exex_monsterlevelenabledflags && A_CheckMonsterLevel(20,2,true,self)) { A_SetTics(3); }
			}
			HADE E 0 
			{
				A_GetHSHealthPreExp();
				A_Die("HSExplode");
			}
			stop;
			
		Pain:
			Goto See;
			
		Death.HSExplode:
		Death:
			HADE E 0 Bright A_Noblocking();
			HADE E 0 Bright A_SpawnProjectile("HSBoomOverlord",0,0,0,0);
			TNT1 A 0 A_UnSetInvulnerable();
			TNT1 A 5;
			Stop;
		XDeath:
			TNT1 A 0 A_UnSetInvulnerable();
			HADE N 0 Bright A_Noblocking();
			HADE H 5 Bright A_StartSound("monster/haddth");
			HADE GFE 4 Bright;
			HADE RSTUVWX 4 Bright;
			Stop;
		Ice:
			goto Death;
	}
}
